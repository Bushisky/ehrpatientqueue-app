<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>${project.parent.artifactId}</id>
	<name>${project.parent.name}</name>
	<version>${project.parent.version}</version>
	<package>${project.parent.groupId}.${project.parent.artifactId}</package>
	<author>HealthIT</author>
	<description>
		${project.parent.description}
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/${project.parent.artifactId}/update.rdf</updateURL>
	<require_version>${openMRSVersion}</require_version>
	<!-- / Module Properties -->

	
	<!-- Required Modules -->
	<require_modules>

		<require_module version="${uiframeworkModuleVersion}">
		org.openmrs.module.uiframework
		</require_module>
																											
		<require_module version="${uilibraryVersion}">
			org.openmrs.module.uilibrary
		</require_module>

		<require_module version="${mchappVersion}">
			org.openmrs.module.mchapp
		</require_module>
        
        <require_module version="${appframeworkVersion}">
            org.openmrs.module.appframework
        </require_module>
		
		<require_module version="${hospitalcoreVersion}">
			org.openmrs.module.hospitalcore
		</require_module>

		<require_module version="17.0.4">
			org.openmrs.module.kenyaemr
		</require_module>

		<require_module version="${appuiVersion}">
			org.openmrs.module.appui
		</require_module>
<!--
		<require_module version="2.11.0-SNAPSHOT">
			org.openmrs.module.referenceapplication
		</require_module>
-->
			
	</require_modules>
	<!-- / Required Modules -->
	
	<!-- Module Activator -->
	<activator>${project.parent.groupId}.${project.parent.artifactId}.PatientQueueAppActivator</activator>
	
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<privilege>
		<name>App: Access MCH Triage</name>
		<description>Able to access the MCH Triage App</description>
	</privilege>
	<privilege>
		<name>App: Access MCH Clinic</name>
		<description>Able to access the MCH Clinic App</description>
	</privilege>
	<privilege>
		<name>App: Access MCH Immunization</name>
		<description>Able to access the MCH Immunization App</description>
	</privilege>

	<!-- All message codes should start with ${project.parent.artifactId}. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

